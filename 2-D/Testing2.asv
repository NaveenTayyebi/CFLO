figure(2)
sinB2 = @(M) ((g-3)*M^2+(g+1)*M^4+(16*g*M^4+...
    ((g-3)*M^2+(g+1)*M^4)^(2))^(1/2))/(4*g*M^4); 
H = fplot(sinB2,'linewidth',1,'color','red');

betaVec = asin((H.YData).^(1/2));
SL = betaVec -asin(((1+(g-1)/2*(H.XData).^2.*(H.YData))...
    ./(g*(H.XData).^2.*(H.YData)-(g-1)/2)).^(1/2)); 




xlim([1 5]);
ylim([0 1.5])
title('Sonic Line');
xlabel('Upstream Mach Number ( M_{1} )');
